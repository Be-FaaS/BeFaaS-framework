{
  "services": {
    "redis": {}
  },
  "program": {
    "functions": {
      "trafficsensorfilter": {
        "provider": "tinyfaas",
        "calls": ["movementplan"]
      },
      "weathersensorfilter": {
        "provider": "tinyfaas",
        "calls": ["roadcondition"]
      },
      "roadcondition": {
        "provider": "tinyfaas",
        "calls": ["setlightphasecalculation"]
      },
      "emergencydetection": {
        "provider": "tinyfaas",
        "calls": ["setlightphasecalculation"]
      },
      "trafficstatistics": {
        "provider": "tinyfaas"
      },
      "setlightphasecalculation": {
        "provider": "tinyfaas"
      },
      "getlightphasecalculation": {
        "provider": "tinyfaas"
      },
      "objectrecognition": {
        "provider": "tinyfaas",
        "calls": ["trafficstatistics", "movementplan", "emergencydetection"]
      },
      "movementplan": {
        "provider": "tinyfaas",
        "calls": ["setlightphasecalculation"]
      }
    }
  }
}
