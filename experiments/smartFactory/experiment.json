{
  "services": {
    "publisherGoogle": {},
    "publisherAws": {},
    "publisherAzure": {},
    "redisGoogle": {},
    "workload": {
      "config": "./workload.yml"
    }
  },
  "program": {
    "functions": {
      "orderSupplies": {
        "provider": "google",
        "call": "async",
        "calls": ["panelOrder", "cushionOrder"]
      },
      "panelOrder": {
        "provider": "azure",
        "call": "async",
        "calls": ["panelProduction"]
      },
      "cushionOrder": {
        "provider": "aws",
        "call": "async",
        "calls": ["cushionProduction"]
      },
      "panelProduction": {
        "provider": "azure",
        "call": "async",
        "calls": ["billing"]
      },
      "cushionProduction": {
        "provider": "aws",
        "call": "async",
        "calls": ["billing"]
      },
      "billing": {
        "provider": "google",
        "call": "async",
        "calls": ["payment"]
      },
      "payment": {
        "provider": "google",
        "call": "async"
      }
    }
  }
}
